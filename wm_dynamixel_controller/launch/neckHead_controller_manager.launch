<!-- -*- mode: XML -*- -->

<launch>
    <node name="dynamixel_manager" pkg="dynamixel_controllers" type="controller_manager.py" required="true" output="screen">
        <rosparam>
            namespace: dxl_manager
            serial_ports:
                neckHead_port:
                    port_name: "/dev/usb-ports/1-1.4"
                    baud_rate: 57142
                    min_motor_id: 1
                    max_motor_id: 50
                    update_rate: 20
        </rosparam>
    </node>
</launch>

